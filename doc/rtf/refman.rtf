\}\par
}
\par
You must implement these two fonctions respectively\par
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid void buildMap(std::shared_ptr<std::vector<std::string>> = nullptr);\par
void setMap(std::shared_ptr<std::vector<std::string>>);\par
}
\par
These fonctions will generate your map in your lib.\par
{\pard\plain \s61\li360\widctlpar\ql\adjustright \fs20\cgrid Keep in mind that some lib does'nt need buildMap, but this fonction will be called BEFORE setMap \par
}You need to implement in entryPoint\par
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid extern "C"\par
\{\par
    IGraphic *entryPoint(void)\par
    \{\par
        YourClassName *instance = new YourClassName();\par
        return (instance);\par
    \}\par
\}\par
}
\par
{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid {\tc\tcl4 SFML Example} \par}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid #include "ClassSFML.hpp"\par
\par
ClassSFML::ClassSFML():\par
    _wind(nullptr),\par
    _key(0),\par
    _isNewPathConfig(false),\par
    _isNewMap(false),\par
    _isNewKey(false)\par
\{\par
    _wind = std::make_unique<sf::RenderWindow>();\par
    _wind->create(sf::VideoMode(SCREEN_WIDTH, SCREEN_HEIGHT), "Arcade SFML");\par
    _wind->setPosition(SCREEN_POS);\par
\}\par
\par
ClassSFML::~ClassSFML()\par
\{\par
    _wind->close();\par
\}\par
\par
void ClassSFML::displayGame()\par
\{\par
    for (auto it = _map->begin(); it != _map->end(); ++it)\par
        for (auto it_sprite = it->begin(); it_sprite != it->end(); ++it_sprite)\par
            if (it_sprite->first != NOTHING)\par
                _wind->draw(it_sprite->second);\par
\}\par
\par
bool ClassSFML::getEvent()\par
\{\par
    while (_wind->pollEvent(_event)) \{\par
        if (_event.type == sf::Event::Closed) \{\par
            _wind->close();\par
            return (true);\par
        \}\par
        if (_event.type == sf::Event::KeyPressed) \{\par
            translateKey();\par
            setIsNewKey(true);\par
        \}\par
    \}\par
    return (false);\par
\}\par
\par
bool ClassSFML::runGraph()\par
\{\par
    if (getIsNewPathConfig() == true) \{\par
        _parsing.clearData();\par
        setIsNewPathConfig(false);\par
        _parsing.setFilename(getPathConfig());\par
        _parsing.readFile();\par
        setMapTexture();\par
    \}\par
    if (!_wind->isOpen())\par
        return (true);\par
    if (getEvent())\par
        return (true);\par
    if (getIsNewMap()) \{\par
        setMapTexture();\par
        _wind->clear();\par
        setIsNewMap(false);\par
    \}\par
    _wind->clear();\par
    displayGame();\par
    _wind->display();\par
    return (false);\par
\}\par
\par
void ClassSFML::setMapTexture()\par
\{\par
    std::vector<DataParsingConfig> parsingResult = _parsing.getResult();\par
    float x = 0;\par
    float y = 0;\par
\par
    _textures.clear();\par
    for (auto it = parsingResult.begin(); it != parsingResult.end(); ++it) \{\par
        sf::Vector2i size = \{it->sizeX, it->sizeY\};\par
        std::shared_ptr<sf::Texture> tmp (new sf::Texture);\par
